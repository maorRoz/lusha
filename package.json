{
  "name": "lusha",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8000",
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "@nestjs/common": "^7.2.0",
    "@nestjs/core": "^7.2.0",
    "@nestjs/mongoose": "^7.0.1",
    "@nestjs/platform-express": "^7.2.0",
    "@nestjs/swagger": "^4.5.12",
    "axios": "^0.19.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "crypto": "^1.0.1",
    "mongoose": "^5.9.24",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "styled-components": "^5.1.1",
    "swagger-ui-express": "^4.1.4",
    "typescript": "^3.9.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.6",
    "@testing-library/user-event": "^12.0.11",
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.4",
    "@types/mongoose": "^5.7.31",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/styled-components": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "concurrently": "^5.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:server": "nest build -p tsconfig.server.json",
    "start:server": "nest start",
    "start:client": "react-scripts start",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:client\"",
    "dev:server": "nest start --debug 5000 --watch",
    "dev:client": "react-scripts start",
    "build:client": "react-scripts build",
    "format": "prettier --write \"{src,server}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,server}/**/*.{ts,tsx}\" --fix",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  }
}
